{
  "name": "@repo/proto",
  "version": "0.0.0",
  "scripts": {
    "pregenerate": "if [ -d ./generate ]; then rm -rf ./generate; fi && mkdir -p ./generate",
    "generate": "grpc_tools_node_protoc --js_out=import_style=commonjs,binary:./generate --grpc_out=grpc_js:./generate --ts_out=grpc_js:./generate service.proto"
  },
  "devDependencies": {
    "grpc-tools": "^1.12.4",
    "grpc-web": "^1.5.0",
    "grpc_tools_node_protoc_ts": "^5.3.3",
    "ts-protoc-gen": "^0.15.0"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.11.1",
    "@types/google-protobuf": "^3.15.12",
    "google-protobuf": "^3.21.4"
  }
}
